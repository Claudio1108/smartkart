<% provide(:title, "AddMarket") %>

<%= current_manager.name %>

<%= form_for @market do |f| %>


    <%= f.label :marketname %><br>
    <%= f.text_field :group %> <br>

    <%= f.label :address %><br>
    <%= f.text_field :address %> <br>

    <%= f.label :informations %><br>
    <%= f.text_field :info %> <br>
 
    <%= f.submit "add market", class: "btn btn-primary" %>

<%end%>
